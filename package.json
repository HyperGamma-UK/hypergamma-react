{
  "name": "Hypergamma",
  "version": "0.0.0",
  "author": "Garrett Flynn <garrettmflynn@gmail.com>",
  "description": "Performance monitoring using fNIRS data",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "workspaces": [
    "services/**"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "tinybuild",
    "tailwind": "npx tailwindcss -i ./tailwindcss/global.css -o ./app/styles/tailwind.css --minify",
